        <text type="lettera" xml:id="lettera_41">
            <!-- TRASCRIZIONE -->
            <body>
                <div type="fronte">
                    <div type="stamp">
                        <ab>
                            <stamp xml:lang="#GE" type="print" facs="#kriegsg_41A1">Kriegsgenfangenenpost</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#corr_41A1">Correspondance des prisonniers de guerre</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#postk_41A1" rend="Bold">Postkarte</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#carte_41A1">Carte postale</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#besetzes_41A1" rend="Bold">Besetztes Gebiet</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#terro_41A1">Territoire occupé</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#unbestzes_41A1" rend="Bold">Unbesetztes Gebiet</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#terrno_41A1">Territoire non occupé</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#nichtz_41A1">Nichtzutreffendes streichen</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#biffer_41A1">Biffer les mentions inutiles</stamp>
                        </ab>
                    </div>
                    <div type="francobolli_e_timbri">
                        <ab>
                            <stamp facs="#timbro1_41A1" type="postmark">
                                <mentioned>scritture illeggibili dentro al timbro</mentioned>
                                <date when="1944-08-14">14.8.44</date>
                                <seg facs="#seg_41A1">Italia Sud</seg>
                        </stamp>
                            <stamp facs="#timbro2_41A1" type="postmark">
                                <mentioned>timbro del campo con bordi viola "Stalag IIIA 51 geprüft", leggibile</mentioned>
                        </stamp>
                        </ab>
                    </div>
                    <div type="destinatario">
                        <ab>
                            <stamp xml:lang="#GE" type="print" rend="Bold" facs="#gebuh_41A1">Gebührenfrei!</stamp>
                            <stamp xml:lang="#IT" type="print" facs="#franco_41A1">Franc de port!</stamp>
                            <address>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#label_nome1_41A1">An</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#label_nome2_41A1">A</stamp>
                                    <persName ref="#LM" facs="#nome_41A1"><addName type="honorific">Signorina</addName><name>Liana Martini</name></persName>
                                </addrLine>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#label_localita1_41A1">Empfangsort:</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#label_localita2_41A1">Lieu de destination</stamp>
                                    <placeName ref="#SE" facs="#localita_41A1">Selvatelle</placeName>
                                </addrLine>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#label_via1_41A1">Straße:</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#label_via2_41A1">Rue</stamp>
                                    <placeName ref="#SE" facs="#via_41A1">Selvatelle</placeName>
                                </addrLine>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#label_provincia1_41A1">Land:</stamp>
                                    <stamp xml:lang="#GE" type="print" facs="#label_provincia2_41A1">Landesteil (Provinc usw.)</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#label_provincia3_41A1">Département</stamp>
                                    <placeName ref="#PI" facs="#provincia_41A1">Pisa</placeName>
                                    <placeName ref="#TO" facs="#regione_41A1">Toscana</placeName>
                                    <hi rend="underline" facs="#stato_41A1">Italia</hi>
                                </addrLine>
                            </address>
                        </ab>
                    </div>
                    <div type="mittente">
                        <ab>
                            <stamp rend="Bold" xml:lang="#GE" type="print" facs="#mit_absender_41A1">Absender:</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#mit_expediteur_41A1">Expéditeur:</stamp>
                            <address>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#mit_label_nome1_41A1">Vor-und Zuname:</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#mit_label_nome2_41A1">Nom et prénom:</stamp>
                                    <persName ref="#AlP" facs="#mit_nome_41A1">Pacini Alberto</persName>
                                </addrLine>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#mit_label_num1_41A1">Gefangenennummer:</stamp>
                                    <idno type="prigioniero" facs="#mit_num_41A1">158767</idno>
                                    <stamp xml:lang="#FR" type="print" facs="#mit_label_num2_41A1">No du prisoner</stamp>
                                </addrLine>
                                <addrLine>
                                    <stamp xml:lang="#GE" type="print" facs="#mit_label_campo1_41A1">Lager-Bezeichnung:</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#mit_label_campo2_41A1">Nom du camp</stamp>
                                    <stamp rend="Bold" xml:lang="#GE" type="print" facs="#mit_campo1_411A">siehe Rüchseite</stamp>
                                    <stamp xml:lang="#FR" type="print" facs="#mit_campo2_41A1">voir au dos</stamp>
                                </addrLine>
                                <addrLine>
                                    <stamp type="print" facs="#mit_stato_41A1"><placeName ref="#GE">Deutschland (Allemagne)</placeName></stamp>
                                </addrLine>
                            </address>
                        </ab>
                    </div>
                    <div type="intestazione">
                        <ab>
                            <stamp xml:lang="#GE" type="print" facs="#stamp1_41A2">Hier abtrennen!</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#stamp2_41A2">Détacher le long du pointillé</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#stamp3_41A2">Diese Seite is fut die Angehörigen des Kriegsgenfangenen bestimmt. Detulich auf die Zeilen schreiben!</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#stamp4_41A2">Ce côté est réservé aux proches parents du prisonnier de guerre! N'écrire que sur les lignes et lisiblment</stamp>
                        </ab>
                    </div>
                </div>
                <div type="retro">
                    <div type="intestazione">
                        <ab>
                            <stamp xml:lang="#GE" type="print" facs="#stamp1_41B1">Kriegsgenlangenenlager</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#stamp2_41B1">Camp des prisonniers</stamp>
                            <stamp type="print" facs="#campo_41B1"><placeName ref="#LU">M.-Stammlager III A</placeName></stamp>
                        </ab>
                    </div>
                    <div type="messaggio">
                        <lb n="1"/>
                        <opener>
                            <dateline>
                                <stamp xml:lang="#GE" type="print" facs="#label_data1_41B1">Datum:</stamp><date when="1944-08-06" facs="#data_41B1">6/8/1944</date>
                                <stamp xml:lang="#FR" type="print" facs="#label_data2_41B1">Date</stamp>
                            </dateline>
                            <lb n="2"/>
                            <salute facs="#salute_41B1"><persName ref="#LM">Liana</persName> adorata.</salute>
                        </opener>
                        <ab>
                            <anchor facs="#iniziomessaggio_41B1" type="start_message"/>Sono le 16 del pomeriggio, è domenica, e
                            <lb facs="#riga2_41B1" n="3"/>come sempre ricordo con grande nostalgi i bei tempi
                            <lb facs="#riga3_41B1" n="4"/>trascorsi. Sai la vita è poco monotona, questi
                            <lb facs="#riga4_41B1" n="5"/>reticolati infastidirebbero chiunque. Per fortuna ho
                            <lb facs="#riga5_41B1" n="6"/>da mangiare in abbondanza altrimenti il mio morale
                            <lb facs="#riga6_41B1" n="7"/>cadrebbe. Cocca cara, sappi che io ti amo sempre di
                            <lb facs="#riga7_41B1" n="8"/>più e che presto ritornerò, per questo fatti ancora coraggio.
                            <lb facs="#riga8_41B1" n="9"/>Tanti bacioni e abbracci <choice><abbr>aff.si</abbr><expan>affettuosi</expan></choice>
                        </ab>
                        <closer>
                            <signed facs="#signed_41B1">il tuo marito<choice><abbr rend="underline">Berto</abbr><expan><persName ref="#AlP">Alberto</persName></expan></choice></signed>
                        </closer>
                    </div>
                    <div type="stamp">
                        <ab>
                            <stamp xml:lang="#GE" type="print" facs="#kriegsg_41B2">Kriegsgenfangenenpost</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#corr_41B2">Correspondance des prissonniers de guerre</stamp>
                            <stamp xml:lang="#GE" type="print" facs="#antw_41B2" rend="Bold">Antwort-Postkarte</stamp>
                            <stamp xml:lang="#FR" type="print" facs="#carte_41B2" rend="Bold">Carte postale de réponse</stamp>
                        </ab>
