                                        <placeName ref="#SE" facs="#via_32A2">Selvatelle</placeName>
                                    </addrLine>
                                    <addrLine>
                                        <stamp xml:lang="#GE" type="print" facs="#label_kreis_32A2">Kreis:</stamp>
                                        <stamp xml:lang="#FR" type="print" facs="#label_arrondisement_32A2">Arrondisement</stamp>
                                    </addrLine>
                                    <addrLine>
                                        <stamp xml:lang="#GE" type="print" facs="#label_provincia1_32A2">Landesteil (Provinc usw.)</stamp>
                                        <stamp xml:lang="#FR" type="print" facs="#label_provincia2_32A2">Dèpartement</stamp>
                                        <placeName ref="#PI" facs="#provincia_32A2">Pisa</placeName>
                                        <placeName ref="#TO" facs="#regione_32A2">(Toscana)</placeName>
                                    </addrLine>
                                    <addrLine>
                                        <placeName ref="#IT" facs="#stato_32A2">ITALIA</placeName>
                                    </addrLine>
                                </address>
                            </ab>
                        </div>
                        <div type="mittente">
                            <ab>
                                <stamp xml:lang="#GE" type="print" facs="#mit_absender_32A2">Absender:</stamp>
                                <stamp xml:lang="#FR" type="print" facs="#mit_expediteur_32A2">Expediteur</stamp>
                                <address>    
                                    <addrLine>
                                        <stamp xml:lang="#GE" type="print" facs="#mit_label_nome1_32A2">Vor-und zuname</stamp>
                                        <stamp xml:lang="#FR" type="print" facs="#mit_label_nome2_32A2">Nom et prènom</stamp>
                                        <persName ref="#AlP" facs="#mit_nome_32A2">Pacini Alberto</persName>
                                    </addrLine>
                                    <addrLine>
                                        <stamp xml:lang="#GE" type="print" facs="#mit_label_num1_32A2">Gefangenennummer:</stamp>
                                        <stamp xml:lang="#FR" type="print" facs="#mit_label_num2_32A2">No. du prisonnier</stamp>
                                        <idno type="prigioniero" facs="#mit_num_32A2">158767</idno>
                                    </addrLine>
                                    <addrLine>
                                        <stamp xml:lang="#GE" type="print" facs="#mit_label_campo1_32A2">Lager-Bezeichnung:</stamp>
                                        <stamp xml:lang="#FR" type="print" facs="#mit_label_campo2_32A2">Designation du camp</stamp>
                                        <stamp><placeName ref="#LU" facs="#mit_campo_32A2">M.-Stammlager III A</placeName></stamp>
                                    </addrLine>
                                    <addrLine>
                                        <stamp rend="bold underline" facs="#mit_stato_32A2"><placeName ref="#GE">Deutschland (Germania)</placeName></stamp>
